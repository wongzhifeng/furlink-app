{
  "name": "furlink-web",
  "version": "1.0.0",
  "description": "FurLink Web Frontend - Pet Emergency Platform - Ultra Performance Optimized",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080 --mode development",
    "build": "NODE_ENV=production vite build",
    "build:analyze": "NODE_ENV=production vite build --mode analyze",
    "preview": "vite preview --port 8080 --host 0.0.0.0",
    "start": "npm run build && npm run preview",
    "build:prod": "NODE_ENV=production vite build",
    "analyze": "npm run build:analyze && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist node_modules/.vite",
    "preinstall": "npx only-allow npm",
    "postinstall": "npm run type-check"
  },
  "dependencies": {
    "axios": "^1.3.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-v8": "^0.30.0",
    "@vitest/ui": "^0.30.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.21",
    "terser": "^5.44.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^0.30.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "not ie 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/hangzhou_thousand_army_wangzhifeng/furlink.git",
    "directory": "frontend/web"
  },
  "homepage": "https://furlink-app.zeabur.app",
  "bugs": {
    "url": "https://gitee.com/hangzhou_thousand_army_wangzhifeng/furlink/issues"
  },
  "license": "MIT",
  "keywords": [
    "furlink",
    "pet",
    "emergency",
    "react",
    "typescript",
    "vite",
    "frontend",
    "web",
    "performance",
    "optimized"
  ],
  "author": {
    "name": "FurLink Team",
    "email": "team@furlink.com"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/furlink"
  },
  "files": [
    "dist",
    "src",
    "public",
    "index.html",
    "vite.config.ts",
    "package.json",
    "README.md"
  ],
  "sideEffects": false,
  "private": false
}
